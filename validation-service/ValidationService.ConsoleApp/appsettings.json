{
  "Workers": {
    "Quotes": {
      "Url": "https://api.hgbrasil.com/weather",
      "Time": 1000
    }
  },
  "Kafka": {
    "Consumers": {
      "Customer": {
        "BootstrapServers": "localhost:9092",
        "GroupId": "gid-customer-validation",
        "EnableAutoCommit": true,
        "AutoOffSetReset": 0,
        "EnablePartionEof": true,
        "Topic": "Customer"
      }
    },
    "Producers": {
      "CustomerValidation": {
        "BootstrapServers": "localhost:9092",
        "Topic": "CustomerValidation"
      }
    }
  }
}