{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Workers": {
    "Quotes": {
      "Url": "https://api.hgbrasil.com/weather",
      "Time": 10000
    }
  },
  "Kafka": {
    "Consumers": {
      "Quotes": {
        "BootstrapServers": "localhost:9092",
        "GroupId": "gid-quotes",
        "EnableAutoCommit": true,
        "AutoOffSetReset": 0,
        "EnablePartionEof": true,
        "Topic": "quotes"
      },
      "CustomerNotification": {
        "BootstrapServers": "localhost:9092",
        "GroupId": "gid-customer-notification",
        "EnableAutoCommit": true,
        "AutoOffSetReset": 0,
        "EnablePartionEof": true,
        "Topic": "CustomerNotification"
      }
    },
    "Producers": {
      "Quotes": {
        "BootstrapServers": "localhost:9092",
        "Topic": "quotes"
      }
    }
  }
}
